   -
     code : |1+
      
      
   -
     code : |1+
      xCadre est un Cadre // Pour l'ajout de cadre dans le tableur
      
      // On ajoute des données dans le tableur
      TableurAjouteDonnée(TBLR_Tableur,1,1,"Référence")
      TableurAjouteDonnée(TBLR_Tableur,2,1,"Libellé produit")
      TableurAjouteDonnée(TBLR_Tableur,3,1,"Description")
      TableurSelectPlus(TBLR_Tableur,"A1","C1")
      TableurStyleSélection(TBLR_Tableur,tblrGras,Vrai)
      
      // On ajoute des cadres
      TableurSelectPlus(TBLR_Tableur,"A1","C1")
      xCadre..Epaisseur = 2
      xCadre..Couleur = Noir
      
      
      TableurCadreSélection(TBLR_Tableur, xCadre)
      TableurSelectPlus(TBLR_Tableur,"A2","C9")
      xCadre..Epaisseur = 0.5
      xCadre..Couleur = Noir
      TableurCadreSélection(TBLR_Tableur, xCadre)
      
      TableurSelectPlus(TBLR_Tableur,"A2","A9")
      xCadre..Epaisseur = 0.5
      xCadre..Couleur = Noir
      TableurCadreSélection(TBLR_Tableur, xCadre)
      
      TableurSelectPlus(TBLR_Tableur,"B2","B9")
      xCadre..Epaisseur = 0.5
      xCadre..Couleur = Noir
      TableurCadreSélection(TBLR_Tableur, xCadre)
      
      TableurSelectPlus(TBLR_Tableur,"C2","C9")
      xCadre..Epaisseur = 0.5
      xCadre..Couleur = Noir
      TableurCadreSélection(TBLR_Tableur, xCadre)
      
      
      // on charge des produits dans le champ
      //on ouvre le fichier pour le lire
      HOuvre(PRODUIT)
      //on se positionne sur le premier enregistrement du fichier
      HLitPremier(PRODUIT)
      POUR i = 1 A 8
      	TableurAjouteDonnée(TBLR_Tableur,1,i+1,PRODUIT.Reference)
      	TableurAjouteDonnée(TBLR_Tableur,2,i+1,PRODUIT.LibProd)
      	TableurAjouteDonnée(TBLR_Tableur,3,i+1,PRODUIT.Description)
      	//on continue de parcourir le fichier pour alimenter le champ
      	HLitSuivant(PRODUIT)
      FIN
      //on ferme le fichier que l'on a lu
      HFerme(PRODUIT)
      
      
      TableurStyleSélection(TBLR_Tableur,tblrTaillePolice,12)
      
      TableurSelectMoins(TBLR_Tableur)
      // On augmente la taille des références
      TableurSelectPlus(TBLR_Tableur,"A2","A9")
      TableurStyleSélection(TBLR_Tableur,tblrTaillePolice,16)
